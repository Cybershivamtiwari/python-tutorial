#while loop
i=1
while i<=20:
    print(i)
    i+=1
print("done with loop")



#guess game
secret_word = "giraffe"
guess = ""
guess_count=0
guess_limit=3
out_of_guess = False
while guess != secret_word:
    
    if guess_count<guess_limit: 
        guess=input("enter the guess:")
        guess_count+=1
    else:
        out_of_guess=true
if out_of_guess:
    print("out of guess , you lose!")
    
else:   
    print("you win!")
    
    
    
    
